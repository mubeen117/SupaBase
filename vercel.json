{
  "version": 2,
  "build": {
    "env": {
      "NEXT_PUBLIC_ENV_VARIABLE": "value"
    }
  },
  "routes": [
    {
      "src": "/_next/static/(.*)",
      "dest": "/_next/static/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
